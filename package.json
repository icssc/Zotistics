{
  "name": "sparkle-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "sst bind vite dev",
    "format": "prettier --write .",
    "postinstall": "husky install && prisma generate && svelte-kit sync",
    "lint": "eslint --quiet --fix .",
    "preview": "vite preview",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev",
    "test": "pnpm test:integration && pnpm test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "bits-ui": "^0.18.1",
    "clsx": "^2.1.0",
    "cmdk-sv": "^0.0.13",
    "svelte-radix": "^1.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "vaul-svelte": "^0.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@commitlint/types": "18.4.3",
    "@playwright/test": "1.40.0",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/kit": "1.27.6",
    "@types/lint-staged": "13.2.2",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "autoprefixer": "^10.4.17",
    "aws-cdk-lib": "2.110.1",
    "constructs": "10.3.0",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-svelte": "2.35.1",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "postcss": "^8.4.35",
    "prettier": "3.1.0",
    "prettier-plugin-packagejson": "2.4.6",
    "prettier-plugin-svelte": "3.1.2",
    "sst": "2.36.3",
    "svelte": "4.2.7",
    "svelte-check": "3.6.1",
    "svelte-kit-sst": "2.36.3",
    "tailwindcss": "^3.4.1",
    "tslib": "2.6.2",
    "typescript": "^5.2.0",
    "vite": "4.5.0",
    "vitest": "0.34.6"
  }
}